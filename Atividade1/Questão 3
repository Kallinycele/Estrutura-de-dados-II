#include <stdio.h>

int main(void) {
  int valor1,valor2, i;
  printf("\nInforme o primeiro valor:\t");
  scanf("%d",&valor1);
  printf("\nInforme o segundo valor:\t");
  scanf("%d",&valor2);

  if(valor1<valor2){
    printf("\nO primeiro valor é menor que o segundo valor\n");
    printf("Sequência:\n");
    for(i=valor1; i<=valor2; i++){
      printf("%d\n",i);
    }
  }
  if(valor1>valor2){
    printf("\nO primeiro valor é maior que o segundo valor\n");
    printf("Sequência:\n");
    for(i=valor1; i>=valor2; i--){
      printf("%d\n",i);
      }
  }
  if(valor1==valor2){
    printf("Valores iguais");
  }
  
  return 0;
}
